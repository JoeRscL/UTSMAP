<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Warna biru tombol -->
    <solid android:color="#0D47A1" />  <!-- Bisa ganti ke warna biru mockup kamu -->

    <!-- Rounded corners -->
    <corners android:radius="12dp" />

    <!-- Sedikit shadow / efek 3D -->
    <padding
        android:left="12dp"
        android:top="12dp"
        android:right="12dp"
        android:bottom="12dp" />

</shape>
